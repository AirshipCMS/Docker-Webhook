{{ $watchUnits := getenv "ETCD_UNITS" }}
[
  null
  {{ range gets $watchUnits }}
  {{ $unitData := json .Value }}
  {{ $version := json $unitData.version }}
  ,{
    "path" : "{{ .Key }}",
    "unit" : "{{ $unitData.unit }}",
    "version" : {{ $version }},
    "type" : "{{ $unitData.type }}",
    "host" : "{{ $unitData.host }}",
    "ipv4_addr" : "{{ $unitData.ipv4_addr }}",
    "port" : "{{ $unitData.port }}"
  }
  {{ end }}
]
