{{ $watchUnits := getenv "ETCD_UNITS" }}
[
  null
  {{ range gets $watchUnits }}
  {{ $unitData := json .Value }}
  {{ $version := json $unitData.version }}
  ,{
    "path" : "{{ .Key }}",
    "unit" : "{{ $unitData.unit }}",
    "version" : {
      "API_VERSION_CODE":"{{ $version.API_VERSION_CODE }}",
      "VERSION_CODE":"{{ $version.VERSION_CODE }}",
      "VERSION_TAG":"{{ $version.VERSION_TAG }}",
      "API_VERSION_TAG":"{{ $version.API_VERSION_TAG }}"
    },
    "type" : "{{ $unitData.type }}",
    "host" : "{{ $unitData.host }}",
    "ipv4_addr" : "{{ $unitData.ipv4_addr }}",
    "port" : "{{ $unitData.port }}"
  }
  {{ end }}
]
